<form method="post" action="{{ path('app_setting_delete', {'id': setting.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer le paramètre {{ setting.id }} ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ setting.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
